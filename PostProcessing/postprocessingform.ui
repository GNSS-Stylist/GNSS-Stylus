          <height>16</height>
         </rect>
        </property>
        <property name="text">
         <string>&lt;html&gt;&lt;head/&gt;&lt;body&gt;&lt;p&gt;These are performed in object's coordinate system as defined in Settings-&amp;gt;Location/orientation solver.&lt;/p&gt;&lt;/body&gt;&lt;/html&gt;</string>
        </property>
